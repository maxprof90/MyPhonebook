<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.maxprof90.myphonebook">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:icon="@mipmap/ic_icon_phonebook"
        android:roundIcon="@mipmap/ic_icon_phonebook_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyPhonebook">
        <activity android:name=".activities.AddContactsActivity" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>